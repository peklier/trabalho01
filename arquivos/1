DROP TABLE semáforo ;
DROP TABLE cidade ;


CREATE TABLE Semaforo (
    numero_semaforo int PRIMARY KEY,
    rua_semaforo int,
    bairro_semaforo int,
    cidade_semaforo int,
    quant_sema_vinc int,
    tempo_fechado int
);

CREATE TABLE Cidade (
    cod_cidade int PRIMARY KEY,
    nome_cidade varchar(50)
);

